<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet"
      integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">

    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
<div class="container-fluid py-4">
  <div class="d-flex logos mx-auto my-2">
    <img class="img-flex mx-auto" width="100" src="/images/pokeball.png" alt="logo_pokeball">
    <img class="img-flex w-25 mx-auto" src="/images/pokedex_logo.png" alt="logo_pokedex">
  </div>
  <div class="pokedex m-auto">
    <div class="left-side">
      <div class="d-flex">
        <div class="circle-top-blue m-4"></div>
        <div class="m-4 d-flex">
          <div id="circle-1" class="mini-circles-top mx-1"></div>
          <div id="circle-2" class="mini-circles-top mx-1"></div>
          <div id="circle-3" class="mini-circles-top mx-1"></div>
        </div>
      </div>
      <div class="pokedex-screen m-auto" id="pokedex-screen">
        <img id="pokemon" class="m-auto">
      </div>
      <div class="d-flex">
        <div class="circle-botton ms-4 mt-4"></div>
        <div class="m-4 d-flex">
          <div id="botton1" class=" mx-1 mini-circles-botton"></div>
          <div id="botton2" class="mx-1 mini-circles-botton"></div>

        </div>
      </div>
      <div class="botton-bar m-auto"></div>
    </div>
    <div class="rigth-side">
      <div id="rigth-bar-top" class="botton-bar mx-auto mt-5 pt-5"></div>
      <div id="rigth-bar-botton" class="d-flex flex-wrap botton-bar mx-auto mt-3">
        <div class="keyboard"></div>
        <div class="keyboard"></div>
        <div class="keyboard"></div>
        <div class="keyboard"></div>
        <div class="keyboard"></div>
        <div class="keyboard"></div>
        <div class="keyboard"></div>
        <div class="keyboard"></div>
        <div class="keyboard"></div>
        <div class="keyboard"></div>
      </div>
      <div class="d-flex mt-5">
        <div class="d-flex mx-auto">
          <div class="rigth-button-rect"></div>
          <div class="rigth-button-rect"></div>
        </div>
        <div id="rcircle-gold" class="circle-botton ms-4 m-auto"></div>
      </div>
      <div class="d-flex mt-3 text-white text-center">


        <div id="rbleft-botton" class="rigth-button-rect m-auto hover">

          <strong><span>anterior</span></strong>
        </div>
        <div id="rbrigth-botton" class="rigth-button-rect m-auto hover">
          <strong><span>siguiente</span></strong>

        </div>

      </div>
    </div>



  </div>
</div>




<script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.9.2/dist/umd/popper.min.js"
  integrity="sha384-IQsoLXl5PILFhosVNubq5LC7Qb9DXgDA9i+tQ8Zj3iwWAwPtgFTxbJ8NT4GN1R8p" crossorigin="anonymous"></script>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.min.js"
  integrity="sha384-cVKIPhGWiC2Al4u+LWgxfKTRIcfu0JTxR+EQDz/bgldoEyl4H0zUF0QKbrJ0EcQF" crossorigin="anonymous"></script>
<script src="/javascripts/index.mjs"></script>
  </body>
</html>
